        <section id="galerie-statica" class="contain-section">
          <h2>Galerie</h2>
          <div id="grid-galerie">
              <% 
                  var vLuni=["ianuarie", "februarie", "martie", "aprilie", "iunie", "iulie"];
                  var d=new Date();
                  var lunaCurenta=vLuni[d.getMonth()];
                  for (let imag of locals.imagini) { 
                      if (imag?.luni?.includes(lunaCurenta)) {
                      %>
                          <figure>
                              <picture>
                                  <source srcset="<%= imag.fisier_mediu %>" media="(max-width:1200px)"/>
                                  <img src="<%= imag.fisier %>" alt="prajituri">
                              </picture>
                          
                          <figcaption><%= imag.descriere%></figcaption>
                          </figure>
                  <% } %>
              <% } %>
          </div>
